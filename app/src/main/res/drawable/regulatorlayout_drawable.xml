<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:endColor="#feefe8" android:startColor="#feefe8" android:angle="90"></gradient>
            <corners android:radius="3dp"></corners>
            <stroke android:color="#eeeeee" android:width="1dp"></stroke>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:endColor="#E4B4B4" android:startColor="#E4B4B4" android:angle="90" />
            <corners android:radius="3dp"></corners>
            <stroke android:color="#eeeeee" android:width="1dp"></stroke>
        </shape>
    </item>
</selector>